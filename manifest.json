{
  "name": "Dota 2 Upcoming Matches",
  "description": "Get a full list of upcoming and outgoing matches of Dota 2.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["scripting"],
  "host_permissions": ["http://localhost:8888/*", "https://dota-pro-upcoming-matches-api.netlify.app/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}
